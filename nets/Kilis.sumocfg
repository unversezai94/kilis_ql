<?xml version="1.0" encoding="iso-8859-1"?>
<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="http://sumo.sf.net/xsd/sumoConfiguration.xsd">
    <input>
        <net-file value="Kilis_static.net.xml"/>
        <route-files value="Kilis.rou.xml"/>
    </input>
    <time>
        <begin value="000"/>
        <end value="90000"/>
    </time>
	<processing>
     <step-method.ballistic value="true"/>
    </processing>
    <time-to-teleport value="-1"/>
	<report>
     <verbose value="true"/>
     <no-step-log value="true"/>
	 <log-file value="my_out.xml"/>
     <tripinfo-output value="my_intersection.trips.xml"/>
   </report>
</configuration>